{
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.2"
    }
  },
  "routes": [
    { "src": "/notifications", "dest": "/api/index.php" },
    { "src": "/image.jpeg", "dest": "/image.jpeg" },
    { "src": "/manifest.json", "dest": "/manifest.json" },
    { "src": "/(.*)", "dest": "/api/index.php" }
  ]
}
